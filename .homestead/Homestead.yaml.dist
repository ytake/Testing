---
ip: "192.168.10.10"
memory: 4096
cpus: 1
provider: virtualbox

authorize: ~/.ssh/id_rsa.pub

keys:
    - ~/.ssh/id_rsa

folders:
    - map: /path/to/your/Testing/application
      to: /home/vagrant/Code/application

sites:
    - map: homestead.app
      to: /home/vagrant/Code/application/public

databases:
    - homestead

variables:
    - key: APP_ENV
      value: local

